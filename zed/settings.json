{
  "project_panel": {
    "dock": "left"
  },
  "theme": "Catppuccin Frapp√©",
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "version": "2"
  },
  "format_on_save": "off",
